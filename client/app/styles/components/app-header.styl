.header
  background-color: #ffffff
  width: 100%;
  height: 71px;
  color: #000000;
  display: flex;
  align-items: center;
  justify-content:space-between;
  padding: 0 24px;
  box-shadow: inset 0 -1px 0 0 #E9ECEF;
  @media screen and (max-width 1280px)
    height 48px
    padding 0 20px

  .header-mainLogo
    img
      @media screen and (max-width 1280px)
        height 21px
        width 149px

  .header-createBtn
    @media screen and (max-width 1280px)
      display none
